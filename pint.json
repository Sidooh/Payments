{
    "preset": "laravel",
    "rules": {
        "binary_operator_spaces": {
            "operators": {
                "=>": "align"
            }
        },
        "cast_spaces": "none",
        "function_declaration": {
            "closure_function_spacing": "none"
        }
    },
    "exclude": [
        "_ide_helper.php",
        "_ide_helper_models.php"
    ]
}
